{
  "name": "5rate-star",
  "version": "0.0.4",
  "description": "Simple library for 5-star rating.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hebertcisco/5rate-star.git"
  },
  "keywords": [
    "nestjs",
    "typescript"
  ],
  "author": "Hebert Cisco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hebertcisco/5rate-star/issues"
  },
  "homepage": "https://github.com/hebertcisco/5rate-star#readme",
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@nestjs/common": "^8.4.4",
    "@nestjs/core": "^8.4.4",
    "nest-shared": "^0.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^8.4.4",
    "@types/jest": "27.4.0",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "eslint": "7.32.0",
    "eslint-plugin-jest": "26.0.0",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  }
}
